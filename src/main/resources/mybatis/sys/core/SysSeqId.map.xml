	</delete>
	
	<select id="getByAlias" resultMap="SysSeqId">
		SELECT * FROM sys_seq_id 
		WHERE 
		ALIAS_=#{alias,jdbcType=VARCHAR} and TENANT_ID_=#{tenantId,jdbcType=VARCHAR}
