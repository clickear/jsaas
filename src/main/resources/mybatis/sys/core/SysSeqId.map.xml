	<update id="updateVersion" parameterType="com.redxun.sys.core.entity.SysSeqId">
		UPDATE sys_seq_id SET CUR_DATE_=#{curDate,jdbcType=TIMESTAMP},CUR_VAL=#{newVal,jdbcType=NUMERIC}
