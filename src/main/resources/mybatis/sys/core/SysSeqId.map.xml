		</where>
		<if test="@Ognl@isNotEmpty(orderBySql)">
			ORDER BY ${orderBySql}
		</if>
		<if test="@Ognl@isEmpty(orderBySql)">
			ORDER BY SEQ_ID_ DESC
		</if>
	</select>
	
	<update id="update" parameterType="com.redxun.sys.core.entity.SysSeqId">
